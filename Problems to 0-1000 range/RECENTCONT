#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
        int n;
        cin>>n;
        string a[n];
        for(int i=0;i<n;i++){
            cin>>a[i];
        }
        int j=0,k=0;
        for(int i=0;i<n;i++){
            if(a[i]=="START38")
                j++;
            else
                k++;
        }
        cout<<j<<" "<<k<<endl;
	}
	return 0;
}
