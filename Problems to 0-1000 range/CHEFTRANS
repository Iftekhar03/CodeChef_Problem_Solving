#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
        int a,c,b,i;
	    cin>>a>>b>>c;
        
        if(a+b>c)
            cout<<"TRAIN"<<endl;
        else if(a+b<c)
            cout<<"PLANEBUS"<<endl;
        else    
            cout<<"EQUAL"<<endl;
	}
	return 0;
}
