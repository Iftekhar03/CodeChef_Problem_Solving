#include <bits/stdc++.h>
#include <iostream>       
#include <cstdio>         
#include <cstring>        
#include <cmath>          
#include <vector>        
#include <algorithm>
using namespace std;

int main(void) {
  int t;
  cin>>t;
  while(t--){
    int a,c,b;
    cin>>a>>b>>c;
    if(a>50)cout<<"A"<<endl;
    else if(b>50)cout<<"B"<<endl;
    else if(c>50)cout<<"C"<<endl;
    else 
      cout<<"NOTA"<<endl;
  }
  return 0;
}

